{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"
      ],
      
      "@components/*":[
        "./src/components/*"
      ],

      "@screens/*":[
        "./src/screens/*"
      ],

      "@storage/*":[
        "./src/storage/*"
      ],

      "@theme/*": [
        "src/styles/theme/*"
      ],

      "@utils/*":[
        "./src/utils/*"
      ],
    }
  }
}
